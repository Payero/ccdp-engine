{
  "logging": {
    "config-file": "${CCDP_HOME}/config/log4j.properties", 
    "logs-dir": "${CCDP_HOME}/logs"
  },
  "engine": {
    "resources-check-cycle": 5,
    "do-not-terminate": [
      "i-0fa470f3da73d8ac0", 
      "i-0a1679a0cb4aba5dd"
    ],
    "skip-heartbeats": false,
    "heartbeat-req-secs": 5
  },
  "interface-impls": {
    "connection": {
      "classname": "com.axios.ccdp.impl.connections.amq.AmqCcdpConnectionImpl",
      "broker": "failover://tcp://ax-ccdp.com:61616",
      "main-queue": "CCDP-Engine"  
    },
    "task-allocator": {
      "classname": "com.axios.ccdp.impl.controllers.NumberTasksControllerImpl",
      "allocate": {
        "avg-cpu-load": 80,
        "avg-mem-load": 15,
        "avg-time-load": 2,
        "no-more-than":  5
      },
      "deallocate": {
        "avg-cpu-load": 20,
        "avg-mem-load": 20,
        "avg-time-load": 2
      }
    },
    "resource-manager": {
      "classname": "com.axios.ccdp.impl.cloud.HybridVMControllerImpl"
    },
    "storage": {
      "classname": "com.axios.ccdp.impl.cloud.aws.AWSCcdpStorageControllerImpl"
    },
    "resource-monitor": {
      "classname": "com.axios.ccdp.impl.monitors.LinuxResourceMonitorImpl",
      "units": "MB",
      "docker-url": "http://172.17.0.1:2375"
    },
    "database": {
      "classname": "com.axios.ccdp.impl.db.mongo.CcdpMongoDbImpl",
      "db-host": "ax-ccdp.com",
      "db-port": 27017,
      "db-name": "CCDP",
      "db-resources-table": "ResourceStatus"
    }
  },
  "resource-provisioning": {
    "resources": {
      "DEFAULT": {
        "classname": "com.axios.ccdp.impl.image.loader.DefaultImageLoaderImpl",
        "image-id":"ami-0c82c390543920339",
        "min-number-free-agents": 0,
        "__security-group": "sg-9e30a2ed",
        "security-group": "sg-54410d2f",
        "subnet-id": "subnet-d7008b8f",
        "key-file-name": "aws_serv_server_key",
        "tags": {
            "session-id": "Service-Node", "Name": "SRB-Test-Default"
          },
        "startup-command": [
            "/data/ccdp/ccdp_install.py",
            "-a", 
            "download", 
            "-d", 
            "s3://ccdp-dist/ccdp-engine12A.tgz", 
            "-w", 
            "-t", 
            "/data/ccdp/"
          ],
        "__assignment-command": "/nishome/srbenne/Scripts/test2.sh",
        "assignment-command": "touch ~/hi.txt",
        "_comment":"This is the aws VM path",
        "region": "us-east-1",
        "role-name": "EMR_EC2_DefaultRole",
        "__role-name": "my-Role",
        "proxy-url": "",
        "__proxy-url": "my-url-to-ge/to/the/proxy",
        "proxy-port": -1,
        "__proxy-port": 1234,
        "credentials-file": "/nishome/srbenne/.aws/credentials",
        "__credentials-file": "/home/oeg/.aws/credentials",
        "credentials-profile-name": "default"
      },
      "EC2": {
        "classname": "com.axios.ccdp.impl.image.loader.EC2ImageLoaderImpl",
        "image-id":"ami-0c82c390543920339",
        "__image-id": "ami-00a7bdc4873460f98",
        "min-number-free-agents": 0,
        "__security-group": "sg-9e30a2ed",
        "security-group": "sg-54410d2f",
        "subnet-id": "subnet-d7008b8f",
        "key-file-name": "aws_serv_server_key",
        "tags": {
            "session-id": "Service-Node", "Name": "SRB-Test-EC2"
          },
        "startup-command": [
            "/data/ccdp/ccdp_install.py",
            "-a", 
            "download", 
            "-d", 
            "s3://ccdp-dist/ccdp-engine12A.tgz", 
            "-w", 
            "-t", 
            "/data/ccdp/"
          ],
        "__assignment-command": "/nishome/srbenne/Scripts/test2.sh",
        "assignment-command": "touch ~/hi.txt",
        "_comment":"This is the aws VM path",
        "region": "us-east-1",
        "role-name": "EMR_EC2_DefaultRole",
        "__role-name": "my-Role",
        "proxy-url": "",
        "__proxy-url": "my-url-to-ge/to/the/proxy",
        "proxy-port": -1,
        "__proxy-port": 1234,
        "_credentials-file": "",
        "credentials-file": "/nishome/srbenne/.aws/credentials",
        "__credentials-file": "/home/oeg/.aws/credentials",
        
        "credentials-profile-name": "default"
      },
      "NIFI": {
        "classname": "com.axios.ccdp.impl.image.loader.NifiImageLoaderImpl",
        "image-id": "ami-00960e391c6790e70",
        "min-number-free-agents": 0,
        "security-group": "sg-54410d2f",
        "subnet-id": "subnet-d7008b8f",
        "key-file-name": "aws_serv_server_key",
        "tags": {
            "session-id": "Service-Node", "Name": "Host-Agent"
          },
        "startup-command": [
            "/data/ccdp/ccdp_install.py",
            "-a", 
            "download", 
            "-d", 
            "s3://ccdp-settings/ccdp-engine.tgz", 
            "-w", 
            "-t", 
            "/data/ccdp"
          ],
        "assignment-command": "/nishome/srbenne/Scripts/test.sh",
        "__assignment-command": "/home/oeg/scripts/test.sh",
        "region": "us-east-1",
        "role-name": "my-Role",
        "proxy-url": "my-url-to-ge/to/the/proxy",
        "proxy-port": 1234,
        "credentials-file": "/home/oeg/.aws/credentials",
        "credentials-profile-name": "default"
      },
      "DOCKER": {
        "classname": "com.axios.ccdp.impl.image.loader.DockerImageLoaderImpl",
        "image-id": "payero/centos-7:ccdp",
        "min-number-free-agents": 0,
        "startup-command": [
          	"/data/ccdp/ccdp_install.py",
          	"-a",
          	"download",
          	"-d",
          	"s3://ccdp-dist/ccdp-engine.tgz",
          	"-w",
          	"-t",
          	"/data/ccdp",
          	"-D"
          ],
        "__startup-command": [
            "/data/ccdp/ccdp_install.py",
            "-a", 
            "download", 
            "-d", 
            "/data/ccdp/ccdp-engine.tgz", 
            "${CCDP_HOME}/config/ccdp-config.json",
            "-w", 
            "-t", 
            "/data/ccdp",
            "-D",
            "-n",
            "DOCKER"
          ],
        "__dist-file":"/data/ccdp/ccdp-engine.tgz",
        "credentials-file": "/nishome/srbenne/.aws/credentials",
        "credentials-profile-name": "default"
      }
    }
  }
}
